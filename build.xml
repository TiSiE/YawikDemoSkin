<project name="YAWIK" default="install">
  <target name="install">
    <echo>YawikDemoSkin ${$target}</echo>
    <copy todir="../../config/autoload" overwrite="true">
      <fileset dir="config">
        <include name="**.dist"/>
      </fileset>   
      <mapper type="regexp" from="^(.*\.php)\.dist" to="\1"/>
      <filterchain>
        <replacetokens begintoken="%%" endtoken="%%">
          <token key="yawik.demoskin.piwik" value="${yawik.demoskin.piwik}"/>
        </replacetokens>
      </filterchain>
    </copy>
  </target>
</project>